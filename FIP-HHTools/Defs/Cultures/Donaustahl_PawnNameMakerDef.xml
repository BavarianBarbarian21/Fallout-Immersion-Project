<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!--American Male-->
    <RulePackDef>
        <defName>HHTools_PawnNameMaker_American_Male</defName>
        <rulePack>
            <rulesStrings>
                <li>r_name->[firstName] [lastName]</li>
                <li>firstName(p=10)->[firstNameA]</li>
                <li>lastName(p=10)->[lastNameA]</li>
                <li>firstName->[firstNameB]</li>
                <li>lastName->[lastNameB]</li>
            </rulesStrings>
            <rulesFiles>
                <li>firstNameA->Names/HHTools_Names_American_Male</li>
                <li>lastNameA->Names/HHTools_Names_American_Last</li>
                <li>firstNameB->Names/HHTools_Names_Chinese_Male</li>
                <li>lastNameB->Names/HHTools_Names_Chinese_Last</li>
            </rulesFiles>
        </rulePack>
    </RulePackDef>

