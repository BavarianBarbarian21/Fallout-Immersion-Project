            <li>WestTek_Endurance_A</li>
            <li>WestTek_Endurance_B</li>
            <li>WestTek_Endurance_C</li>
            <li>WestTek_Endurance_D</li>

            <li>WestTek_Charisma_A</li>
            <li>WestTek_Charisma_B</li>
            <li>WestTek_Charisma_C</li>
            <li>WestTek_Charisma_D</li>

            <li>WestTek_Intelligence_A</li>
            <li>WestTek_Intelligence_B</li>
            <li>WestTek_Intelligence_C</li>
            <li>WestTek_Intelligence_D</li>

