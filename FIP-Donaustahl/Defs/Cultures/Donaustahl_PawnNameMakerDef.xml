<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!--American Male-->
    <RulePackDef>
        <defName>Donaustahl_PawnNameMaker_American_Male</defName>
        <rulePack>
            <rulesStrings>
                <li>name->[First] [Last]</li>
            </rulesStrings>
            <rulesFiles>
                <li>First(p=10)->Names/Donaustahl_Names_American_Male</li>
                <li>Last(p=10)->Names/Donaustahl_Names_American_Last</li>
                <li>First(p=1)->Names/Donaustahl_Names_Chinese_Male</li>
                <li>Last(p=1)->Names/Donaustahl_Names_Chinese_Last</li>
            </rulesFiles>
        </rulePack>
    </RulePackDef>

