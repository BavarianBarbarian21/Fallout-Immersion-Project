<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!--American Male-->
    <RulePackDef>
        <defName>Donaustahl_PawnNameMaker_American_Male</defName>
        <rulePack>
            <rulesStrings>
                <li>r_name->[First] [Last]</li>
                <li>First(p=10)->[FirstA]</li>
                <li>Last(p=10)->[LastA]</li>
                <li>First->[FirstB]</li>
                <li>Last->[LastB]</li>
            </rulesStrings>
            <rulesFiles>
                <li>FirstA->Names/Donaustahl_Names_American_Male</li>
                <li>LastA->Names/Donaustahl_Names_American_Last</li>
                <li>FirstB->Names/Donaustahl_Names_Chinese_Male</li>
                <li>LastB->Names/Donaustahl_Names_Chinese_Last</li>
            </rulesFiles>
        </rulePack>
    </RulePackDef>

