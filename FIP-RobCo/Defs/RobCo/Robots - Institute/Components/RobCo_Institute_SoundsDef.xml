    <SoundDef>
        <defName>RobCo_Pacificator_Call</defName>
        <context>MapOnly</context>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <volumeRange>25</volumeRange>
                <pitchRange>0.95~1.05</pitchRange>
                <repeatMode>NeverTwice</repeatMode>
